{
  "name": "react-starter-kit",
  "version": "0.0.1",
  "engines": {
    "node": "6.3.1"
  },
  "scripts": {
    "start": "./node_modules/.bin/nodemon --watch dist -e js,jade ./dist/app.js | bunyan",
    "start-debug": "./node_modules/.bin/nodemon --debug --watch dist -e js,jade ./dist/app.js | bunyan",
    "build-js": "./node_modules/.bin/babel ./src -d ./dist --copy-files",
    "build-js-watch": "./node_modules/.bin/babel --watch ./src --out-dir ./dist",
    "browserify": "./node_modules/.bin/browserify ./src/client/js/main.js -o public/js/main.js -t babelify -t envify -d --extension=.jsx",
    "browserify-watch": "./node_modules/.bin/watchify ./src/client/js/main.js -o public/js/main.js -t babelify -t envify -d --extension=.jsx -v",
    "uglify": "./node_modules/.bin/uglifyjs public/js/main.js -o public/js/main.min.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "postinstall": "mkdir -p public/js && mkdir -p public/css && npm run build-prod",
    "js-watch": "npm run build-js-watch & npm run browserify-watch",
    "build-dev": "npm run build-js && npm run browserify",
    "build-prod": "npm run build-dev && npm run uglify",
    "test": "./node_modules/.bin/jest --config jest.json"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "axios": "0.13.1",
    "babel-cli": "6.8.0",
    "babel-core": "6.9.0",
    "babel-plugin-transform-class-properties": "6.8.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babelify": "7.2.0",
    "bluebird": "3.4.1",
    "body-parser": "1.15.0",
    "browserify": "13.1.0",
    "bunyan": "1.8.1",
    "classnames": "2.2.4",
    "compression": "1.6.1",
    "connect-redis": "3.0.2",
    "dotenv": "2.0.0",
    "envify": "3.4.1",
    "errorhandler": "1.4.3",
    "express": "4.13.4",
    "express-session": "1.13.0",
    "express-validator": "2.20.3",
    "jade": "1.11.0",
    "lodash": "4.8.2",
    "logzio-nodejs": "0.3.8",
    "lusca": "1.3.0",
    "newrelic": "1.28.0",
    "node-sass-middleware": "^0.9.8",
    "passport": "0.3.2",
    "postcss-middleware": "1.1.2",
    "raygun": "0.8.5",
    "react": "15.3.0",
    "react-addons-create-fragment": "15.3.0",
    "react-dom": "15.3.0",
    "react-redux": "4.4.5",
    "react-router": "2.5.2",
    "redial": "0.4.2",
    "redis": "2.6.2",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "throng": "4.0.0",
    "uglify-js": "2.7.0",
    "validator": "5.2.0"
  },
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "babel-jest": "14.1.0",
    "eslint": "2.10.0",
    "eslint-plugin-flow-vars": "0.5.0",
    "eslint-plugin-react": "5.1.1",
    "flow-bin": "0.27.0",
    "jest-cli": "14.1.0",
    "nodemon": "1.9.2",
    "react-addons-test-utils": "15.3.0",
    "watchify": "3.7.0"
  },
  "license": "MIT"
}
